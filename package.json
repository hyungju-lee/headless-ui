{
  "name": "@hyungju-lee/headless-ui",
  "version": "1.0.27",
  "description": "headless-ui",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:lib"
  },
  "keywords": [
    "headless-ui",
    "vue",
    "react",
    "ui framework",
    "ui library",
    "design custom free"
  ],
  "author": "",
  "license": "ISC",
  "main": "dist/",
  "module": "dist/",
  "exports": {
    "./vue2": {
      "import": "./dist/vue2/headless-ui-vue2.es.js",
      "require": "./dist/vue2/headless-ui-vue2.cjs.js",
      "types": "./dist/vue2/types/index.d.ts"
    },
    "./vue2/style.css": {
      "import": "./dist/vue2/vue2.css",
      "require": "./dist/vue2/vue2.css"
    },
    "./vue3": {
      "import": "./dist/vue3/headless-ui-vue3.es.js",
      "require": "./dist/vue3/headless-ui-vue3.cjs.js",
      "types": "./dist/vue3/types/index.d.ts"
    },
    "./vue3/style.css": {
      "import": "./dist/vue3/vue3.css",
      "require": "./dist/vue3/vue3.css"
    },
    "./react18": {
      "import": "./dist/react18/headless-ui-react18.es.js",
      "require": "./dist/react18/headless-ui-react18.cjs.js",
      "types": "./dist/react18/types/index.d.ts"
    },
    "./react18/style.css": {
      "import": "./dist/react18/react18.css",
      "require": "./dist/react18/react18.css"
    },
    "./react19": {
      "import": "./dist/react19/headless-ui-react19.es.js",
      "require": "./dist/react19/headless-ui-react19.cjs.js",
      "types": "./dist/react19/types/index.d.ts"
    },
    "./react19/style.css": {
      "import": "./dist/react19/react19.css",
      "require": "./dist/react19/react19.css"
    }
  }
}
